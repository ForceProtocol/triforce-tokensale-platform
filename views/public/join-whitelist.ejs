<%- partial('./partials/whitelist-header.ejs') %>


<div class="container padding-top60">

	<div class="section">


		<div class="whitelist-form-section col-lg-offset-2 col-md-offset-2 col-sm-offset-1 col-lg-8 col-md-8 col-sm-10 col-xs-12">

			<img class="wow fadeInDown animated" src="/images/header-logo-180.png" alt="TriForce Tokens Ltd Logo" style="visibility: visible; animation-name: fadeInDown;">

			<br />
			<br />
			<br />

			<h1><%- __('Get ready to buy FORCE tokens') %></h1>
			<h4><%- __('become an <u class="green-highlight">approved</u> contributor today') %> <i class="fa fa-check green-highlight"></i></h4>

			<br />



			<div class="whitelist-form-container">

				<div style="display:none;" class="loading-window">
					<span>Processing Please Wait...</span>
					<div class="loading">Loading&#8230;</div>
				</div>
				
				<div class="alert alert-info">
					<h3>Token Sale Now Closed!</h3>
					<p>You may still complete the KYC process if you wish to enter the main sale.</p>
					<p>Full details to be announced soon.</p>
				</div>

				<form id="kycForm" class="text-left whitelist-form" action="">

					<input type="hidden" id="rfrId" name="rfrId" value="<%- req.session.token && req.session.user ? req.session.user.id : '' %>" />
					<input type="hidden" id="validateToken" name="validateToken" value="<%- req.session.token ? req.session.token : '' %>" />

					<div id="scroll-to-warning-message" class="warning-message"></div>


					<div class="form-step" <%- req.session.token && req.session.kyc ? 'style="display:none;"' : '' %> id="form-step-1">

						<p><%- __('Before completing this process you must read and agree to the following:') %></p>

						<br />

						<div class="form-group row">
							<label class="col-sm-2 col-form-label"><%- __('Token Sale Agreement') %></label>
							<div class="col-sm-10">
								<div class="form-check">
									<input required="required" class="form-check-input" type="checkbox" id="agreement1" <%= req.session.token && req.session.kyc ? 'checked=checked' : '' %>>
									<label class="form-check-label form-input-note" for="gridCheck1">
										<%- __('I confirm I have read and understood the <a target="_blank" href="/terms-of-token-sale">terms and conditions</a> of this token sale.') %>
									</label>
								</div>
							</div>
						</div>


						<div class="form-group row">
							<label class="col-sm-2 col-form-label"><%- __('Risk Disclaimer') %></label>
							<div class="col-sm-10">
								<div class="form-check">
									<input required="required" class="form-check-input" type="checkbox" id="agreement2" <%- req.session.token && req.session.kyc ? 'checked=checked' : '' %>>
									<label class="form-check-label form-input-note" for="gridCheck1">
										<%- __('I understand that buying TriForce Tokens (FORCE) involves substantial risk of loss and is not suitable for every contributor. The valuation of the TriForce Tokens may fluctuate, and, as a result, you as an contributor could lose your full contribution. Before deciding to participate, you should carefully consider your objectives, level of experience, and risk appetite.') %>
									</label>
								</div>
							</div>
						</div>

						<div class="form-group row">
							<div class="col-lg-4 col-md-4 col-sm-6 col-xs-8 col-lg-offset-4 col-md-offset-4 col-sm-offset-3 col-xs-offset-2">
								<button id="continue-step-1" type="button" class="btn btn-green-highlight pull-right btn-block"><%- __('Continue') %> &raquo;</button>
							</div>
						</div>


					</div>


					<div class="form-step" style="display:none;" id="form-step-2">
					
						<div class="form-group row">
							<label for="inputEmail3" class="col-sm-2 col-form-label"><%- __('Your Gender') %></label>

							<div class="col-sm-10">
								<label class="input-gender-label">
									<input required="required" name="gender" type="radio" class="input-gender" value="male">
									<span><%- __('Male') %></span>
								</label>
								<label class="input-gender-label">
										<input required="required" name="gender" type="radio" class="input-gender" value="female">
										<span><%- __('Female') %></span>
								</label>
							</div>

						</div>

						<div class="form-group row">
							<label for="inputEmail3" class="col-sm-2 col-form-label"><%- __('Your Date of Birth') %></label>
							<div class="col-sm-3">
								<select id="inputDayOfBirth" required="required" class="form-control" name="monthOfBirth">
									<option value=""><%- __('DAY') %></option>
									<% for(var d = 1;d < 32;d++){
										if(d < 10){
											outputD = '0' + d;
										}else{
											outputD = d;
										}
									%>
									<option value="<%= outputD %>"><%= outputD %></option>
									<% } %>
								</select>
							</div>
							<div class="col-sm-3">
								<select id="inputMonthOfBirth" required="required" class="form-control" name="monthOfBirth">
									<option value=""><%- __('MONTH') %></option>
									<% for(var m = 1;m < 13;m++){
										if(m < 10){
											outputM = '0' + m;
										}else{
											outputM = m;
										}
									%>
									<option value="<%= outputM %>"><%= outputM %></option>
